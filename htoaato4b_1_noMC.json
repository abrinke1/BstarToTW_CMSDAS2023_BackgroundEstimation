            "ALIAS": "Run2_data",
            "LOC": "path/FILE:HIST"
        },
        "SIGNAME": {
            "SYSTEMATICS":["lumi"],
            "SCALE": 1.0, 
            "COLOR": 1, 
            "TYPE": "SIGNAL",
            "TITLE": "HToAATo4B_M-35",
            "LOC": "path/FILE:HIST"
        }
    },
    "SYSTEMATICS": {
        "lumi": {
            "CODE": 0,
            "VAL": 1.018
        }
    },
    "BINNING": {
        "default": {
            "X": {
            "NAME": "mH",
            "TITLE": "m_{H} [GeV}",
            "MIN": 70,
            "MAX": 270,
            "NBINS": 20,
            "SIGSTART": 100,
            "SIGEND": 140
            },
            "Y": {
            "NAME": "mA",
            "TITLE": "m_{A} [GeV]",
            "MIN": 10,
            "MAX": 64,
            "NBINS": 27
            }
        }
    },
    "OPTIONS": {
        "blindedPlots": ["Pass"],
        "blindedFit": ["Pass"],
        "plotUncerts": false,
        "plotTitles": false,
        "year": 1
