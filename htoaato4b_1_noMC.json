{
    "NAME": "HToAATo4B",
    "GLOBAL": {
        "help": "The GLOBAL object contains meta information regarding the location, filenames, and input histogram names for all ROOT files used in the background estimation procedure. As well, the SIGNAME list should include the name(s) of all signals you wish to investigate.",
        "FILE": "$process.root",
        "FILE_UP": "$process.root",
        "FILE_DOWN": "$process.root",
        "HIST": "hLeadingFatJetParticleNet_massH_Hto4b_avg_vs_massA_Hto4b_avg_$region",
        "HIST_UP": "hLeadingFatJetParticleNet_massH_Hto4b_avg_vs_massA_Hto4b_avg_$region$systup",
        "HIST_DOWN": "hLeadingFatJetParticleNet_massH_Hto4b_avg_vs_massA_Hto4b_avg_$region$systdown",
        "pathLPC": "/eos/cms/store/user/ssawant/htoaa/analysis/20240131_GGFMode_DataVsMC/2018/2DAlphabet_inputFiles",
        "path": "/eos/cms/store/user/ssawant/htoaa/analysis/20240131_GGFMode_DataVsMC/2018/2DAlphabet_inputFiles",
        "SIGNAME": ["SUSY_GluGluH_01J_HToAATo4B_M-35_HPtAbv150"]
    },
    "REGIONS": {
        "Pass": {
            "PROCESSES": ["SIGNAME"],
            "BINNING": "default"
        },
        "Fail": {
                "PROCESSES": ["SIGNAME"],
                "BINNING": "default"
        }
    },
    "PROCESSES": {
        "data_obs": {
            "SYSTEMATICS": [],
            "SCALE": 1.0,
            "COLOR": 1,
            "TYPE": "DATA",
            "TITLE": "Data",
